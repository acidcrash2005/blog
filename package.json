{
    "name": "with-typescript",
    "version": "1.0.0",
    "scripts": {
        "dev": "next",
        "build": "(yarn lint) && (yarn gen:types) && next build",
        "start": "(yarn lint) && (yarn next)",
        "prod": "next start ./src",
        "type-check": "tsc",
        "gen:types": "graphql-codegen --config codegen.yml",
        "typescript": "tsc",
        "lint": "yarn tsc && yarn lint:eslint",
        "lint:eslint": "eslint 'src/pages/**/*.{ts,tsx}'",
        "prettier:ts": "prettier-eslint --list-different $PWD/'src/pages/**/*.{ts,tsx}';prettier-eslint --write $PWD/'pages/**/*.{ts,tsx}'",
        "format": "yarn prettier:ts"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@apollo/react-ssr": "^3.1.3",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.16",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.3",
        "isomorphic-unfetch": "3.0.0",
        "next": "latest",
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.13.0",
        "@graphql-codegen/introspection": "1.13.0",
        "@graphql-codegen/typescript": "1.13.0",
        "@graphql-codegen/typescript-operations": "1.13.0",
        "@graphql-codegen/typescript-react-apollo": "1.13.0",
        "@types/node": "^12.12.21",
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "babel-plugin-graphql-tag": "^2.5.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "prettier-eslint-cli": "^5.0.0",
        "typescript": "3.7.3"
    },
    "license": "ISC"
}
